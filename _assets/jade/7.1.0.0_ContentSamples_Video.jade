include _mixins.jade
 
doctype html
html(lang="es")
  head
    +head("ComparTips")
  body
    .wrapperRegistered
        +sidebar
        .fullDocument
            header.mainHeader.innerHeader 
              +headerRegistered
              .hrdots
            .container
                .mainTitle.tag.purple Mi mente 
                .mainTitle.purple CONFERENCIA VIRTUAL: A PARADIGM SHIFT
                .contenidoDeArticulo
                  .rowArticulo
                    .fullWidth.purple
                        .ranking(class="r_3")
                            - for(var i=0; i<5;i++)
                             .star
                        .texto 
                            p <strong>Riesgo de ACV vs Riesgo de Sangrado, encontrando el punto de equilibrio.</strong>
 
                            p Conferencia virtual en el marco del VII Congreso Internacional de Neurología, en Bogotá, Colombia.
 
                            p Dr John Smith, PhD - Hardvard University.
                  .rowArticulo
                    .fullWidth
                        .texto
                            img(src="_assets/img/imgvideo.png", alt="")
                +sharing
                
                .hrshadow
                
                .secTitle.purple.nomobile OTRAS COMIDAS QUE PUEDEN INTERESARTE
            section.module.purple.nomobile
                .container
                    section.notes
                        +nota("_assets/img/notas/04.png", "4", "hidratarme al hacer ejercicio")
                        +nota("_assets/img/notas/05.png", "1", "Test de una vida sedentaria")
                        +nota("_assets/img/notas/06.png", "", "Tips de los expertos")
                        .clear
                .clear
            .clear
            

            


            script(src="_assets/js/_libs/jquery.js")
            script(src="_assets/js/_libs/sidebar.js")
            script(src="_assets/js/_libs/slideme2.js")
            script(src="_assets/js/_libs/jquery.tooltipster.min.js")
            script(src="_assets/js/_script.js")
            footer
                +footer

 